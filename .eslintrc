{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "strict": 0,
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0,
    "react/prefer-stateless-function": 0,
    "react/forbid-prop-types": 1,
    "new-cap": 0
  },
  "globals": {
    "document": true
  },
  "ignore": [
    "vendors/*",
    "bundles/*",
    "builds/*"
  ]
}